.page-wrapper {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
.user-baseinfo {
  padding: 18rpx 30rpx;
  margin: auto;
  text-align: center;
  background-color: #fff;
}
.avatar {
  margin: 0 auto 10rpx;
  width: 150rpx;
  height: 150rpx;
  border-radius: 100%;
  overflow: hidden;
}
.avatar image {
  width: 100%;
  height: 100%;
}
.info-wrapper {
  padding: 10rpx 30rpx 0;
  margin-top: 20rpx;
  margin-bottom: 10rpx;
  background-color: #fff;
}
.info-item {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  border-bottom: 1rpx solid #f1f1f1;
}
.info-left {
  float: left;
}
.info-right {
  float: right;
  color: #888;
}
.info-item:last-child {
  border-bottom: 0rpx;
}

.info-name {
  margin-left: 30rpx;
  font-size: 26rpx;
  color:#999;
}
.huizhang {
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  width: 60rpx;
  height: 60rpx;
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 100% 100%;
}
.wrapper2 {
  margin-top: 10rpx;
}
.icon-right {
  position: relative;
  top: 5rpx;
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 100% 100%;
}