.wrapper {
  margin-top: 200rpx;
  padding: 30rpx;
}
.item-line {
  margin-bottom: 80rpx;
}
.item {
  display: inline-block;
  margin: 0 54rpx;
  text-align: center;
  font-size: 30rpx;
  color: #666;
}
.item-avatar {
  margin-bottom: 10rpx;
  width: 120rpx;
  height: 120rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.item-avatar > image {
  width: 100%;
  height: 100%;
}