.info-wrapper {
  padding: 10rpx 30rpx 0;
  margin-top: 20rpx;
  margin-bottom: 10rpx;
  background-color: #fff;
}
.info-item {
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
  font-size: 32rpx;
  border-bottom: 1rpx solid #f1f1f1;
}
.info-left {
  float: left;
}
.info-right {
  float: right;
  color: #888;
}
.icon-right {
  position: relative;
  top: 8rpx;
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 100% 100%;
}